@model EMS.Models.PresenterTimeSlot

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>PresenterTimeSlot</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="StartTime" class="control-label"></label>
                <input asp-for="StartTime" class="form-control" />
                <span asp-validation-for="StartTime" class="text-danger"></span>
            </div>
      
            <div class="form-group">
                <label asp-for="PresenterSectorId" class="control-label"></label>
                <select asp-for="PresenterSectorId" class="form-control" asp-items="ViewBag.PresenterSectorId"></select>
                <span asp-validation-for="PresenterSectorId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-controller="Presenter" asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
