@model EMS.Models.PresenterTimeSlot

@{
    ViewData["Title"] = "Create";
}

<div class="row justify-content-center ">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group mb-5">
                <label asp-for="StartTime" class="control-label"></label>
                <input asp-for="StartTime" class="form-control" />
                <span asp-validation-for="StartTime" class="text-danger"></span>
            </div>
      
            <div class="form-group mb-5">
                <label asp-for="PresenterSectorId" class="control-label"></label>
                <select asp-for="PresenterSectorId" class ="form-control" asp-items="ViewBag.PresenterSectorId"></select>
            </div>
            <div class="form-group mb-5">
                <input type="submit" value="Create" class="doc_border_btn doc_border_btn_two sec-btn-blue mt-2 btn btn-primary w-100 " />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
